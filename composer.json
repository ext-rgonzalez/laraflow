{
  "name": "ext-rgonzalez/laraflow",
  "description": "Laravel workflow package for Eloquent models.",
  "license": "MIT",
  "authors": [
    {
      "name": "Ricardo Gonzalez",
      "email": "rgonzalez2520@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1.20",
    "illuminate/support": "^v8.83.27"
  },
  "require-dev": {
    "mockery/mockery": "^0.9.6",
    "phpunit/phpunit": "^6.0 || ^7.0 || ^8.0 || ^9.0"
  },
  "autoload": {
    "psr-4": {
      "laraflow\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    },
    "laravel": {
      "providers": [
        "laraflow\\LaraflowServiceProvider"
      ]
    }
  }
}
